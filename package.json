  "scripts": {
    "start": "webpack serve --env goal=3000",
    "build:prod": "webpack --env mode=production",
    "build:dev": "webpack --env mode=development",
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "analyze:prod": "webpack --env mode=production analyze=true",
    "analyze:dev": "webpack --env mode=development analyze=true",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook"
  },
